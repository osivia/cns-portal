<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
version="1.0">

	<portlet>
		<description>Portlet CNS services Directory feeder</description>
		<portlet-name>cns-services-ldapFeederPortlet</portlet-name>
        <display-name>cns services LDAP feeder</display-name>


        <portlet-class>com.osivia.cns.proto.ldap.CNSFeederServicePortlet</portlet-class>
		
		<init-param>
			<name>contextConfigLocation</name>
			<value>classpath:annuaire-beans.xml
				/WEB-INF/applicationContext.xml</value>
		</init-param>

		<supports>
			<mime-type>text/html</mime-type>
		</supports>
		<portlet-info>
			<title>Portlet CNS services Directory feeder</title>
		</portlet-info>
	</portlet>
	
	
</portlet-app>